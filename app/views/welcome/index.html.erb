<div class="container">
  <div class="row selector-nav">
    <div class='col-md-12 top-bar'>
      <div class='entry_form'>
        <span> Zipcode:</span>
        <input type='text' id='zipo' value='98102' />
        <input type='submit' class="btn btn-large btn-success" id='mysubmit1' value='Go' />
        <span> Region:</span>
        <span id='hardregion'></span>
      </div>
    </div>
  </div>
</div>

<br>
<div class="container">
  <div class="outputs">
    <div class="row">
      <div class='col-md-5 left-side'>
        <table id='plants' class="table table-striped table-bordered"
               cellspacing="0" width="100%">
          <thead><th>Plant Name</th></thead>
          <tbody>
            <% @plants.each do |p| %>
            <tr id='<%= p.id %>'>
              <td>
                <%= p.plant_name %>
              </td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      <div id='plants_details' class='col-md-7 right-side'>
        This is the right side of the page showing plant detail.
      </div>
    </div>
  </div>
</div>
